<h1>Add Song for <%= link_to @artist.name, @artist %>:</h1>

<%= form_for ([@artist, @song]) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :duration %>
  <%= f.number_field :duration %>

  <%= f.label :audio_url %>
  <%= f.text_field :audio_url %>

  <%= f.submit "Create" %>
<% end %>
